{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/get_location_names",
      "dest": "/server.py"
    },
    {
      "src": "/predict_home_price",
      "dest": "/server.py"
    }
  ]
}
 