{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.10"
      }
    }
  ],
  "routes": [
    {
     "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}






